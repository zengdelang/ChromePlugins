{
  "name": "AdBlock for CSDN",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts":[
  {
      "matches":["http://blog.csdn.net/*"],
      "js":["jquery-1.8.2.min.js","adblocker.js"],
      "run_at": "document_end"
  }
  ],
    "permissions": [
    "tabs", "http://blog.csdn.net/*"
  ]
  
}
